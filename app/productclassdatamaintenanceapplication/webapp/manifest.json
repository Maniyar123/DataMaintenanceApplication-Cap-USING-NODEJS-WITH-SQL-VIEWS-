{
  "_version": "1.59.0",
  "sap.app": {
    "id": "com.productclassdatamaintenanceapplication",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.14.4",
      "toolsId": "290ca01d-91b2-40da-b3ff-17351f07e34c"
    },
    "dataSources": {
      "mainService": {
        "uri": "/v2/odata/v4/catalog-service2/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.127.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.productclassdatamaintenanceapplication.i18n.i18n"
        }
      },
      "productclassmodel": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.productclassdatamaintenanceapplication.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteProductClassMainView",
          "pattern": "",
          "target": [
            "TargetProductClassMainView"
          ]
        },
        {
          "name": "ProductCharacteristicsView",
          "pattern": "CharacteristicsView/{productId}",
          "target": [
              "TargetCharacteristicsView"
          ]
      },
      {
        "name": "AddProductView",
        "pattern": "AddProductView",
        "target": [
            "TargetAddProductView"
        ]
    }
       
      ],
      "targets": {
        "TargetProductClassMainView": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "ProductClassMainView",
          "viewName": "ProductClassMainView"
        },
        "TargetCharacteristicsView": {
            "viewType": "XML",
            "transition": "slide",
            "clearControlAggregation": false,
            "viewId": "ProductCharacteristicsView",
            "viewName": "ProductCharacteristicsView"
        },
        "TargetAddProductView": {
            "viewType": "XML",
            "transition": "slide",
            "clearControlAggregation": false,
            "viewId": "AddProductView",
            "viewName": "AddProductView"
        }
      }
    },
    "rootView": {
      "viewName": "com.productclassdatamaintenanceapplication.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}

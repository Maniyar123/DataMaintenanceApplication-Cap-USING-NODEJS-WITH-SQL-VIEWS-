<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
 <Dialog
        id="classDialog"
        title="Class and Characteristic Selection"
        afterOpen="onBeforeOpen"
        draggable="true">
        <VBox id="myVBox" class="sapUiSmallMargin">
            <!-- Panels will be dynamically added here -->
        </VBox>
        <buttons>
        <Button  text="Submit" press="onSubmitClassCharVal"></Button>
            <Button
                text="Close"
                press="onCloseClassCharValDialog"/>

        </buttons>
    </Dialog>
</core:FragmentDefinition>

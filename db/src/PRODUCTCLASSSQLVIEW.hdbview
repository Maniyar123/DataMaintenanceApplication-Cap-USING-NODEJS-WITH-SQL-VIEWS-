VIEW "PRODUCTCLASSSQLVIEW"
AS SELECT 
PRODUCTDETAILSDATACLASS_PRODUCT.PRODUCTNAME,
PRODUCTDETAILSDATACLASS_PRODUCT.PRODUCTID,
PRODUCTDETAILSDATACLASS_PRODUCT.TYPE,
PRODUCTDETAILSDATACLASS_CHARACTERISTIC.CHARACTERISTICNAME,
PRODUCTDETAILSDATACLASS_CHARACTERISTICVALUE.VALUE,
PRODUCTDETAILSDATACLASS_CHARACTERISTICVALUE.VALUEDESCRIPTION,
PRODUCTDETAILSDATACLASS_CLASS.CLASSID
FROM 
PRODUCTDETAILSDATACLASS_PRODUCTCLASS
INNER JOIN PRODUCTDETAILSDATACLASS_PRODUCT
ON PRODUCTDETAILSDATACLASS_PRODUCTCLASS.PRODUCTID_PRODUCTID = PRODUCTDETAILSDATACLASS_PRODUCT.PRODUCTID
INNER JOIN PRODUCTDETAILSDATACLASS_CLASS
ON PRODUCTDETAILSDATACLASS_PRODUCTCLASS.CLASSID_CLASSID = PRODUCTDETAILSDATACLASS_CLASS.CLASSID
INNER JOIN PRODUCTDETAILSDATACLASS_CHARACTERISTIC
ON PRODUCTDETAILSDATACLASS_CHARACTERISTIC.CLASSID_CLASSID = PRODUCTDETAILSDATACLASS_CLASS.CLASSID
INNER JOIN PRODUCTDETAILSDATACLASS_CHARACTERISTICVALUE
ON PRODUCTDETAILSDATACLASS_CHARACTERISTICVALUE.CHARACTERISTICID_CHARACTERISTICID = PRODUCTDETAILSDATACLASS_CHARACTERISTIC.CHARACTERISTICID;